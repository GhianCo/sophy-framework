{
    "name": "ghianco/sophy-framework",
    "description": "Framework Personal PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sophy\\": "src/Sophy"
        },
        "files": [
            "src/Helpers/app.php",
            "src/Helpers/dir_files.php"
        ]
    },
    "scripts": {
        "tests": "phpunit --color=always --testdox -v tests/",
        "php-cs-fixer": "php-cs-fixer fix --diff"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofhy\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "GhianCo",
            "email": "ghiancorx@gmail.com"
        }
    ],
    "require": {
        "php-di/php-di": "6.4",
        "slim/http": "1.1",
        "slim/psr7": "0.3|1.6",
        "slim/slim": "4.11",
        "vlucas/phpdotenv": "5.4",
        "firebase/php-jwt": "6.4.0",
        "mark-gerarts/auto-mapper-plus": "1.4",
        "symfony/console": "5.4",
        "spatie/regex": "1.4",
        "friendsofphp/php-cs-fixer": "3.8",
        "vlucas/valitron": "1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    }
}
